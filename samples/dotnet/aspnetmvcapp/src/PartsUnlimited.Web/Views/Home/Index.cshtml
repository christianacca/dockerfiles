@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1>ASP.NET</h1>
    <p class="lead">ASP.NET is a free web framework for building great Web sites and Web applications using HTML, CSS and JavaScript.</p>
    <p><a href="https://asp.net" class="btn btn-primary btn-lg">Learn more &raquo;</a></p>
</div>

<div class="row">
    <div class="col-md-6">
        <h2>Product categories</h2>
        <p>
            Manage Products
        </p>
        <p>@Html.ActionLink("Manage", "Index", "Products", null, new { @class= "btn btn-default" })</p>
    </div>
    <div class="col-md-6">
        <h2>Rain checks</h2>
        <p>
            Manage Rain checks
        </p>
        <p>@Html.ActionLink("Manage", "Index", "RainChecks", null, new { @class = "btn btn-default" })</p>
    </div>
</div>